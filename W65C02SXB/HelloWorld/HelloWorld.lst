     1                        ; File: HelloWorld.asm
     2                        
     3                        ; This is a simple example of how to turn on LEDs 
                    to represent a basic HelloWorld project in 6502 Assembly
     4                        
     5                        		org	$1900			    ; Th
                    is is the location in memory where the program is stored
     6                        
     7                        ; When the board is plugged in or reset, Port B of
                     the W65C22 device is setup as INPUTS.  
     8                        ; Since we are turning LEDS ON and OFF, we want Po
                    rt B to be OUTPUTS
     9 00:1900: A9 FF        		lda	#$FF				
                    ; LoaD the Accumulator with FF = 1111 1111 (ALL HIGH)
    10 00:1902: 8D C2 7F     		sta	$7FC2				
                    ; STore Accumulator to address $7FC2 which is the Data Direc
                    tion Register B
    11                        							
                    	; This sets each of the Port B lines to be OUTPUTS.  
    12                        
    13                        ; Now that Port B is setup to turn the LEDs ON, we
                     need to tell it which ones.
    14                        		
    15 00:1905: A9 EF        		lda #%11101111	        ; LED OFF
    16 00:1907: 8D C0 7F     		sta $7FC0				
                    ; This is the location of the Output Register of Port B	
                    	
    17                        
    18 00:190A: A9 10        		lda #%00010000          ; LED ON
    19 00:190C: 8D C0 7F     		sta $7FC0				
                    ; This is the location of the Output Register of Port B	
    20                        		
    21 00:190F: 8D C0 7F     		sta $7FC0				
                    ; This is the location of the Output Register of Port B	
    22 00:1912: 4C 05 19     		jmp $1905				
                    ; JuMP to beginning of program
    23                        
    24                        ; End of Lesson		
